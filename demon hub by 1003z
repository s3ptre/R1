-- Notification Sound
local s = Instance.new("Sound", workspace)
 s.Volume  = 50
 s.SoundId = "rbxassetid://570433034"
 s.Looped  = False
 s:Play()
local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("Demon Hub 2023 Latest Version")
local serv = win:Server("Scripts", "")
-- Tabs
local main = serv:Channel("Main")
local sldrs = serv:Channel("character")
local effects = serv:Channel("Effects")
local other = serv:Channel("Other Stuff")
local vaporum = serv:Channel("Vaporum X")
local tgls = serv:Channel("Toggles")
local uitab = serv:Channel("UI")
local tp = serv:Channel("Teleports")
local discord = serv:Channel("Discord")

-- Section 1
main:Button("Slow Farm", function()
_G.farm = true

		while wait() do
			if _G.farm == true then
				for i = 1,10 do
					if game.Players.LocalPlayer.Character:FindFirstChild("sword") then
						game:GetService("Players").LocalPlayer.Character.sword.Handle.up.RemoteEvent:FireServer()
					else
						game:GetService("Players").LocalPlayer.Backpack.sword.Handle.up.RemoteEvent:FireServer()
					end
				end
			end
		end
end)

main:Button("Unslow Farm", function()
 _G.farm = false

		while wait() do
			if _G.farm == true then
				for i = 1,10 do
					if game.Players.LocalPlayer.Character:FindFirstChild("sword") then
						game:GetService("Players").LocalPlayer.Character.sword.Handle.up.RemoteEvent:FireServer()
					else
						game:GetService("Players").LocalPlayer.Backpack.sword.Handle.up.RemoteEvent:FireServer()
					end
				end
			end
		end
end)

main:Button("Afk Farm", function()
_G.active = true

		game:GetService("RunService").Stepped:Connect(function()
			if _G.active == true then
				for i = 1,10 do
					if game.Players.LocalPlayer.Character:FindFirstChild("sword") then
						game:GetService("Players").LocalPlayer.Character.sword.Handle.up.RemoteEvent:FireServer()
					else
						game:GetService("Players").LocalPlayer.Backpack.sword.Handle.up.RemoteEvent:FireServer()
					end
				end
			end
		end)
end)

main:Button("Unafk Farm", function()
_G.active = false

		game:GetService("RunService").Stepped:Connect(function()
			if _G.active == true then
				for i = 1,10 do
					if game.Players.LocalPlayer.Character:FindFirstChild("sword") then
						game:GetService("Players").LocalPlayer.Character.sword.Handle.up.RemoteEvent:FireServer()
					else
						game:GetService("Players").LocalPlayer.Backpack.sword.Handle.up.RemoteEvent:FireServer()
					end
				end
			end
		end)
end)

main:Button("Kill Aura - Old", function()
  --// Setting \\--
local range = 40

--// Variable \\--
local player = game:GetService("Players").LocalPlayer

--// Script \\--
game:GetService("RunService").RenderStepped:Connect(function()
    local p = game.Players:GetPlayers()
    for i = 2, #p do local v = p[i].Character
        if v and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and player:DistanceFromCharacter(v.HumanoidRootPart.Position) <= range then
            local tool = player.Character and player.Character:FindFirstChildOfClass("Tool")
            if tool and tool:FindFirstChild("Handle") then
                tool:Activate()
                for i,v in next, v:GetChildren() do
                    if v:IsA("BasePart") then
                        firetouchinterest(tool.Handle,v,0)
                        firetouchinterest(tool.Handle,v,1)
                    end
                end
            end
        end
    end
end)   
end)

main:Button("Sword Damage Script", function()
local player = game.Players.LocalPlayer
local character = player.Character
local backpack = player.Backpack
local swords = {}
function look(dir)
    for i, v in pairs(dir:GetChildren()) do
        if(v:IsA("Tool"))then
            table.insert(swords,v)
        end
    end
end
look(backpack)
look(character)
    
for i, v in pairs(swords)do
    local sword = v
    sword.Parent = backpack
    sword.Handle.Massless = true
    sword.Handle.CanCollide = true
    sword.Parent = character
    sword.Handle.Touched:Connect(function(t)
        if(t.Parent)and(t.Parent:FindFirstChild("Humanoid"))and(t.Parent.Name~=player.Name)then
            sword.Handle.dmg.RemoteEvent:FireServer(t.Parent.Humanoid, math.huge)
        end
    end)
end    
end)

-- Section 2
local sldr = sldrs:Slider("Walkspeed", 0, 1000, 16, function(s)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s 
end)
sldrs:Button("Default", function()
sldr:Change(16)
end)

local sldr = sldrs:Slider("Jumppower", 0, 1000, 16, function(j)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = j 
end)
sldrs:Button("Default", function()
sldr:Change(16)
end)

sldrs:Button("God Mode", function()
if game.Players.LocalPlayer.Backpack:FindFirstChild("sword") then
		if game.Players.LocalPlayer.Backpack:FindFirstChild("sword") then
			game.Players.LocalPlayer.Backpack.sword.Handle.dmg.RemoteEvent:FireServer(game.Players.LocalPlayer.Character.Humanoid, - math.huge)
			wait(0.4)
		else
			local lplr = game.Players.LocalPlayer.Name 
			while true do
				wait()
				game:GetService('Players').LocalPlayer.Character.Humanoid:EquipTool(game.Players[lplr].Backpack["sword"])
				for i,v in ipairs(game.Players:GetPlayers())do
					game.Workspace[lplr].sword.Handle.dmg.RemoteEvent:FireServer(workspace[v.Name].Humanoid, -)
					game.Players.LocalPlayer.Character.pvp:Destroy()
				end 
			end
		end
	end 
end)


sldrs:Button("Permagod", function()
local plr = game.Players.LocalPlayer
	while wait() do
		spawn(function()
			if plr.Backpack:FindFirstChild("sword") then
				game:GetService("Players").LocalPlayer.Backpack.sword.Handle.dmg.RemoteEvent:FireServer(plr.Character.Humanoid, -math.huge)
			else
				game:GetService("Players").LocalPlayer.Character.sword.Handle.dmg.RemoteEvent:FireServer(plr.Character.Humanoid, -math.huge)
			end
			if plr.Character.Humanoid.Health == plr.Character.Humanoid.MaxHealth then
				if plr.Backpack:FindFirstChild("sword") then
					game:GetService("Players").LocalPlayer.Backpack.sword.Handle.dmg.RemoteEvent:FireServer(plr.Character.Humanoid, -math.huge)
				else
					game:GetService("Players").LocalPlayer.Character.sword.Handle.dmg.RemoteEvent:FireServer(plr.Character.Humanoid, -math.huge)
				end
			end
		end)
	end 
end)


sldrs:Button("Respawn", function()
local plr = game.Players.LocalPlayer
	local pos = plr.Character.HumanoidRootPart.CFrame
	local Humanoid = plr.Character.Humanoid:Clone()
	plr.Character.Humanoid:Destroy()
	Humanoid.Parent = plr.Character
	Humanoid.Health = 0; wait(0.12) plr.Character:Destroy()
	plr.CharacterAdded:Wait()
	wait(0.3)
	plr.Character.HumanoidRootPart.CFrame = pos  
end)

-- Section 3
effects:Button("Red And Blue Flames", function()
  local SumPValues = 0
local amountPlayerCount = 0
for i = 1, 20
    do 
	--color sword
	local Player = game.Players.LocalPlayer
	for i = 1, 10 do
			local v1 = "Red"
		local event = game:GetService("Workspace").eff.RemoteEvent
		event:FireServer(v1)
		
		local v1 = "Blue"
		local event = game:GetService("Workspace").eff.RemoteEvent
		event:FireServer(v1)
	end
	wait(0.0001)
	--create sword
	local event = game:GetService("Workspace").load.RemoteEvent
	event:FireServer()
	wait(0.18)
	--put tools in hand
	for i, v in pairs(Player.Backpack:GetChildren()) do
		v.Parent = Player.Character
	end
	--drop tools and add tag
	for i, v in pairs(Player.Character:GetChildren()) do
		if v:IsA('Tool') then
			v.Parent = workspace
			ex = Instance.new('Script')
			ex.Parent = v
			ex.Name = 'MineYesMine'
			for ix, vx in pairs(game.Players:GetChildren()) do
				if vx.Character:FindFirstChild('HumanoidRootPart') ~= nil then
					if vx.Character.HumanoidRootPart.Position.X - v.Handle.Position.X < 20 and vx.Character.HumanoidRootPart.Position.X - v.Handle.Position.X > -20 then
						if vx ~= game.Players.LocalPlayer then
							xXPos = vx.Character.HumanoidRootPart.Position.X - v.Handle.Position.X
							print(v.Handle.Position.X)
						end
					end
				end
			end
		end
	end
end
-- pick up tagged tools

-- remove tags
for it, vt in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
	if vt:FindFirstChild('MineYesMine') ~= nil then
		vt.MineYesMine:Destroy()
	end
end
end)

